apiVersion: cert-manager.io/v1
kind: ClusterIssuer
metadata:
  name: acme2dfn-issuer
spec:
  acme:
    server: http://acme2file-service

    email: "test@example.com"

    privateKeySecretRef:
      name: acme-account-private-key

    solvers:
      - http01:
          ingress:
            class: nginx
